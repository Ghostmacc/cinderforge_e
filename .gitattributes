* text=auto
*.py   text eol=lf
*.md   text eol=lf
*.toml text eol=lf
*.yml  text eol=lf
*.yaml text eol=lf
*.ps1  text eol=crlf
*.png  binary
*.jpg  binary
*.pdf  binary
*.pt          filter=lfs diff=lfs merge=lfs -text
*.safetensors filter=lfs diff=lfs merge=lfs -text
*.bin         filter=lfs diff=lfs merge=lfs -text
